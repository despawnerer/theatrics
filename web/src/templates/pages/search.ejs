<div class="search-view content-container">
  <div class="search-feed"
       data-items-container=".search-results"
       data-no-items-message=".no-results"
       data-loader=".big-loader-container">
    <ol class="search-results" hidden></ol>
    <div class="nothing-at-all no-results" hidden>Ничего не найдено 😞</div>
    <div class="nothing-at-all no-query" hidden>Нет запроса — нет ответа 👆</div>
    <%= bigLoader() %>
  </div>
</div>
